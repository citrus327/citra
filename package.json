{
  "name": "@citrus327/citra",
  "version": "0.0.0",
  "description": "My awesome typescript library",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "citra": "./bin/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:base": "tsup src/index.ts --format cjs,esm --dts -d ./dist",
    "build:cli": "tsup src/cli/index.ts --format cjs,esm --dts -d ./dist/cli",
    "dev:base": "pnpm run build:base --watch",
    "dev:cli": "pnpm run build:cli --watch",
    "dev": "run-p dev:* -n -l -c",
    "test": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@egoist/prettier-config": "^1.0.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.2"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "vite": "^5.2.12"
  }
}
